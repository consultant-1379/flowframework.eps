MODULE sliding_window_INTERNAL_SYSTEM_UTILIZATION;

CREATE VARIABLE INTEGER var_window_length_INTERNAL_SYSTEM_UTILIZATION = 4;

@Name('sliding_window_INTERNAL_SYSTEM_UTILIZATION')
SELECT
	* 
FROM
	INTERNAL_SYSTEM_UTILIZATION.std:groupwin(resourceId).ext:sort(var_window_length_INTERNAL_SYSTEM_UTILIZATION, ropId desc)
OUTPUT SNAPSHOT EVERY 1 EVENTS;